{
  "name": "@mwarnerdotme/react-remixicon",
  "description": "React component for the RemixIcon icon library",
  "version": "0.0.3",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "homepage": "https://gitlab.com/mwarnerdotme/react-remixicon",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/mwarnerdotme/react-remixicon.git"
  },
  "contributors": [
    "Michael Warner <gitlab.com/mwarnerdotme>"
  ],
  "license": "MIT",
  "scripts": {
    "build": "yarn clean && yarn build:execute && yarn build:post",
    "build:execute": "rollup -c rollup.config.js",
    "build:post": "mv ./dist/dist/* ./dist && rm -rf ./dist/dist",
    "icons:generate": "ts-node ./scripts/generateIconTypescriptFiles.ts",
    "icons:clean": "rm -f ./src/icons/*.ts",
    "dist": "NODE_ENV=production yarn build",
    "lint": "eslint ./src",
    "prepack": "yarn dist",
    "clean": "rm -rf dist"
  },
  "peerDependencies": {
    "react": ">=16.x",
    "react-dom": ">=16.x",
    "remixicon": ">=2.5.0"
  },
  "devDependencies": {
    "@remix-run/eslint-config": "^1.1.3",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@svgr/rollup": "^6.2.1",
    "@types/react": "^17.0.38",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.8.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "react": ">=17.x",
    "react-dom": "^17.0.2",
    "remixicon": "^2.5.0",
    "rollup": "^2.64.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "files": [
    "dist"
  ],
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "ie > 10"
  ]
}
